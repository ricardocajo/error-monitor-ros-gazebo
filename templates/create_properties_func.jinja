{% for prop in properties %}
def property_{{ loop.index }}(states):
    if {{ prop.comparisons[0] }}:
        print(f'{{'{Fore.BLUE}'}}Error at line {{ prop.line }}:{{'{Style.RESET_ALL}'}}\n {{ prop.comparisons[1] }}')
        print('{{'{0}'}}Failing state:{{'{1}'}}\n    {% for var in prop.comparisons[2] -%}{% set index = loop.index+2 -%}}{{var}}: {{'{'}}{{index}}{{'}'}}\n{% endfor -%}'.format(Fore.RED,Style.RESET_ALL{% for var in prop.comparisons[3] -%},{{var}}{% endfor -%}))
        return False
    return True
{% endfor -%}